<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fun.wxy.annoy_o_tron.DevFragment"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_screenshot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Screenshot"/>

    <Button
        android:id="@+id/btn_dev_screenshot2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Screenshot 2 by root"/>

    <Button
        android:id="@+id/btn_start_overlay_service"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Start Overlay Service"/>

    <Button
        android:id="@+id/btn_stop_overlay_service"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Stop Overlay Service"/>

    <Button
        android:id="@+id/btn_start_timer_service"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Start Timer Service"/>

    <Button
        android:id="@+id/btn_stop_timer_service"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Stop Timer Service"/>

    <Button
        android:id="@+id/btn_dev_test2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TEST 2"/>

    <Button
        android:id="@+id/btn_dev_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TEST"/>

    <ImageView
        android:id="@+id/iv_dev_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
